{
    "name": "slickdeals/gearman-messenger",
    "description": "Symfony Gearman Messenger Bridge",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Feaver",
            "email": "brian.feaver@slickdeals.net"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-gearman": ">=2.0",
        "ext-json": "*",
        "symfony/messenger": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/serializer": "^4.4|^5.0",
        "vimeo/psalm": "^4.6",
        "friendsofphp/php-cs-fixer": "^2.18",
        "marcocesarato/php-conventional-changelog": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "SD\\Gearman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SD\\Tests\\Gearman\\": "tests/"
        }
    }
